@using Licenta.SDK.Localization;
@inherits BaseCetComponent<ComponentsResource>
<li class="nav-item">
    <a id="btn-CbsMenu" class="nav-link collapsed" data-bs-target="#cbsMenu-nav" data-bs-toggle="collapse" href="#">
        <i class="bi bi-menu-button-wide"></i><span>CBS</span><i class="bi bi-chevron-down ms-auto"></i>
    </a>
    <ul id="cbsMenu-nav" class="nav-content collapse" data-bs-parent="#sidebar-nav">
        @foreach (var item in MenuItem.Items)
        {
            <li>
                <a id="@item.ElementId" href="@item.Url">
                    <i class="bi bi-circle"></i><span>@Localizer[item.DisplayName]</span>
                </a>
            </li>
        }
    </ul>
</li><!-- End Components Nav -->
