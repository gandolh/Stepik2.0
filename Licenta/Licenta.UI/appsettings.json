{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "LicentaConfig": {
    "UrlApi": "https://LICENTA.API:8081",
    //"UrlApi": "https://localhost:7253",
    "Kafka": {
      "Address": "broker:9092",
      "SubscribeTopics": [ "RunCode-Resp" ],
      "GroupId": "PortalLicenta",
      "Endpoints": {
        "RunCode": "RunCode-Req"
      }
    },
    "Endpoints": {
      "GetCourses": "/Course/GetAll",
      "GetOneCourse": "/Course/GetOne",
      "GetOneLesson": "/Lesson/GetOne",
      "GetCoursesByStudent": "/Course/GetAllByStudent",
      "Register": "/Account/Register",
      "GetUser": "/Account/GetUser",
      "GetCodeEvaluations": "/CodeEvaluationEntry/GetAll",
      "GetExercises": "/Exercise/GetAll",
      "GetLessons": "/Lesson/GetAll",
      "GetModules": "/Module/GetAll",
      "GetQuizVariants": "/QuizVariant/GetAll",
      "GetStudents": "/Student/GetAll",
      "GetTeachers": "/Teacher/GetAll",
      "GetOneCodeEvaluation": "/CodeEvaluationEntry/GetOne",
      "GetOneExercise": "/Exercise/GetOne",
      "GetOneModule": "/Module/GetOne",
      "GetOneQuizVariant": "/QuizVariant/GetOne",
      "GetOneStudent": "/Student/GetOne",
      "GetOneTeacher": "/Teacher/GetOne",
      "UpdateCodeEvaluation": "/CodeEvaluationEntry/Update",
      "UpdateCourse": "/Course/Update",
      "UpdateExercise": "/Exercise/Update",
      "UpdateLesson": "/Lesson/Update",
      "UpdateModule": "/Module/Update",
      "UpdateQuizVariant": "/QuizVariant/Update",
      "UpdateStudent": "/Student/Update",
      "UpdateTeacher": "/Teacher/Update",
      "DeleteCodeEvaluation": "/CodeEvaluationEntry/Delete",
      "DeleteCourse": "/Course/Delete",
      "DeleteExercise": "/Exercise/Delete",
      "DeleteLesson": "/Lesson/Delete",
      "DeleteModule": "/Module/Delete",
      "DeleteQuizVariant": "/QuizVariant/Delete",
      "DeleteStudent": "/Student/Delete",
      "DeleteTeacher": "/Teacher/Delete"
    }
  }
}







