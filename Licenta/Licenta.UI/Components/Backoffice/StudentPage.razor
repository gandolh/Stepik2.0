@page "/crud/student/{ViewMode?}/{Id:int?}";
@using Licenta.UI.Components.Backoffice.Student
@inherits BaseCrudPage

<CrudPageLayout>
    @if (string.IsNullOrEmpty(ViewMode))
    {
        <StudentAll />
    }else if (ViewMode.Contains("one") && Id!= null)
    {
        <StudentOne Id="@((int)Id)" />
    }
</CrudPageLayout>