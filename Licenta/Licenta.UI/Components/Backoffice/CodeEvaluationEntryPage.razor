@page "/crud/codeeval/{ViewMode?}/{Id:int?}";
@inherits BaseCrudPage
@using Licenta.UI.Components.Backoffice.CodeEval

<CrudPageLayout>
        @if (string.IsNullOrEmpty(ViewMode))
    {
        <CodeEvalAll/>
    }else if (ViewMode.Contains("one") && Id!= null)
    {
        <CodeEvalOne Id="@((int)Id)" />
    }
</CrudPageLayout>
