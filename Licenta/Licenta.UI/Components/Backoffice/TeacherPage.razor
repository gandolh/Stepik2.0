@page "/crud/teacher/{ViewMode?}/{Id:int?}";
@using Licenta.UI.Components.Backoffice.Teacher
@inherits BaseCrudPage

<CrudPageLayout>
    @if (string.IsNullOrEmpty(ViewMode))
    {
        <TeacherAll />
   }else if (ViewMode.Contains("one") && Id!= null)
    {
        <TeacherOne Id="@((int)Id)" />
    }
</CrudPageLayout>