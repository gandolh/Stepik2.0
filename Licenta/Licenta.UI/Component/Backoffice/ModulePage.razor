@page "/crud/module/{ViewMode?}/{Id:int?}";
@using Licenta.UI.Component.Backoffice.Module
@inherits BaseCrudPage

<CrudPageLayout>
    @if (string.IsNullOrEmpty(ViewMode))
    {
        <ModuleAll />
    }else if (ViewMode.Contains("one") && Id!= null)
    {
        <ModuleOne Id="@((int)Id)" />
    }
</CrudPageLayout>